{
  "version": 2,
  "builds": [
    {
      "src": "client/**",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "tsc && vite build",
        "outputDirectory": "dist/client"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}